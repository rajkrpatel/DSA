package com.array;

public class IsSubsequence {

	public static void main(String[] args) {
		String s = "abc";
		String t = "agdbsdch";
		
		int count1=0, count2 = 0;
		
		while(s.length()>count1 && t.length()> count2) {
			
			if(s.charAt(count1) == t.charAt(count2)) {
				count1++;
				count2++;
			} else {
				count2++;
			}
		}
		
		if(count1 == s.length()) {
			System.out.println(s + " is subsequence of " + t);
		}

	}

}
